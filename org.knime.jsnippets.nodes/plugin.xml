<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.2"?><plugin>
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/scripting/java-snippet" factory-class="org.knime.jsnippets.nodes.JavaSnippetNodeFactory"  />
      <node category-path="/flowcontrol/variables" factory-class="org.knime.jsnippets.nodes.JavaEditVarNodeFactory"  />
   </extension>
   <extension point="org.knime.core.NodeFactoryClassMapper">
      <NodeFactoryClassMapper classMapper="org.knime.jsnippets.nodes.JavaSnippetNodeFactoryClassMapper" />
   </extension>
   <extension point="org.knime.jsnippets.templaterepository">
         <template-repository id="org.knime.jsnippets.nodes.templates.JavaSnippetFileTemplateRepositoryProvider" provider-class="org.knime.jsnippets.nodes.templates.JavaSnippetFileTemplateRepositoryProvider" />
         <template-repository id="org.knime.jsnippets.nodes.templates.JavaSnippetPluginTemplateRepositoryProvider" provider-class="org.knime.jsnippets.nodes.templates.JavaSnippetPluginTemplateRepositoryProvider" />
   </extension>

</plugin>
